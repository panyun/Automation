                        <TabItem Header="组件库" Visibility="Collapsed">
                            <Grid Margin="20">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="180"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <StackPanel>
                                    <Border Background="White" Height="120" BorderBrush="#FAF7FA" BorderThickness="1" CornerRadius="8">
                                        <Grid Margin="20,0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>
                                                <RowDefinition/>
                                            </Grid.RowDefinitions>
                                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
                                                <TextBlock Text="手动搜索:" Foreground="#333333" FontSize="16" FontFamily="黑体" Margin="0,5,0,0"/>
                                                <Border BorderBrush="#F0EDF1" BorderThickness="1"  Margin="20,0,0,0" CornerRadius="6,0,0,6">
                                                    <TextBox Text="小型" Width="150" Foreground="#999999" Height="25" BorderThickness="0" FontSize="14" Margin="1" Padding="3,3,0,0"/>
                                                </Border>

                                                <Button Style="{StaticResource Search.Button}"/>
                                            </StackPanel>
                                            <StackPanel  Orientation="Horizontal" VerticalAlignment="Center" Grid.Row="1" Margin="0,0,0,10">
                                                <TextBlock Text="所属类目:" Foreground="#333333" FontSize="16" FontFamily="黑体" Margin="0,3,0,0"/>
                                                <RadioButton GroupName="lm" Content="全部" IsChecked="True" Style="{StaticResource Circle.RadioButton}" Margin="20,0,0,0" FontSize="16"/>

                                                <RadioButton GroupName="lm" Content="流程组件"  Style="{StaticResource Circle.RadioButton}" Margin="20,0,0,0" FontSize="16"/>
                                                <RadioButton GroupName="lm" Content="应用软件"  Style="{StaticResource Circle.RadioButton}" Margin="20,0,0,0" FontSize="16"/>
                                                <RadioButton GroupName="lm" Content="系统操作"  Style="{StaticResource Circle.RadioButton}" Margin="20,0,0,0" FontSize="16"/>
                                                <RadioButton GroupName="lm" Content="程序处理"  Style="{StaticResource Circle.RadioButton}" Margin="20,0,0,0" FontSize="16"/>
                                            </StackPanel>                                  
                                        </Grid>
                                    </Border>
                                    <Grid Margin="0,20,0,0">
                                        <ToggleButton  VerticalAlignment="Center" HorizontalAlignment="Left" Style="{StaticResource hzxs}"/>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Right">
                                            <Button Content="导入" Style="{StaticResource Blue.Button}" Width="60" Height="30"/>
                                            <Button Content="一键更新" Style="{StaticResource Blue.Button}" Width="120" Height="30" Margin="20,0,0,0"/>
                                        </StackPanel>
                                    </Grid>
 
          
                                </StackPanel>
                                <DataGrid 
                                    Grid.Row="1"
                                        Margin="0,20,0,0"
                                        GridLinesVisibility="Horizontal"
                                        HorizontalGridLinesBrush="#F2F2F2"
                                        SelectionMode="Extended" 
                                        BorderThickness="0"
                                        BorderBrush="#661472AE"
                                        CanUserResizeColumns="False"
                                        CanUserSortColumns="False"
                                        Style="{StaticResource MetroDataGrid}">
                                    <DataGrid.ItemsSource>
                                        <test:Data1  />
                                    </DataGrid.ItemsSource>
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Binding="{Binding F1}" Header="组件" Width="*"/>
                                        <DataGridTextColumn Binding="{Binding F2}" Header="来源" Width="*"/>
                                        <DataGridTextColumn Binding="{Binding F3}" Header="来源" Width="*"/>
                                        <DataGridTemplateColumn Header="操作" Width="*">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                        <Button Content="更新" Style="{StaticResource Link.Button}"/>
                                                        <Button Content="删除" Style="{StaticResource Link.Button}" Foreground="Red" Margin="10,0,0,0"/>
                                                    </StackPanel>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                        <TabItem Header="依赖库" Visibility="Collapsed">
                            <Grid Margin="20">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <StackPanel Orientation="Horizontal">
                                    <Button Content="批量更新" Style="{StaticResource Blue.Button}" Height="30" Width="120"/>
                                    <Button Content="安装新依赖" Style="{StaticResource Blue.Button}" Height="30" Width="120" Margin="20,0,0,0"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Right">
                                    <Border BorderBrush="#F0EDF1" Background="#F9F9F9" BorderThickness="1"  Margin="20,0,0,0" CornerRadius="6,0,0,6">
                                        <TextBox Background="Transparent" Text="输入搜索关键词" Width="200" Foreground="#999999" Height="25" BorderThickness="0" FontSize="14" Margin="1" Padding="3,3,0,0"/>
                                    </Border>

                                    <Button Style="{StaticResource Search.Button}"/>
                                </StackPanel>


                                <DataGrid 
                                    Grid.Row="1"
                                        Margin="0,20,0,0"
                                        GridLinesVisibility="Horizontal"
                                        HorizontalGridLinesBrush="#F2F2F2"
                                        SelectionMode="Extended" 
                                        BorderThickness="0"
                                        BorderBrush="#661472AE"
                                        CanUserResizeColumns="False"
                                        CanUserSortColumns="False"
                                        Style="{StaticResource MetroDataGrid}">
                                    <DataGrid.ItemsSource>
                                        <test:Data1  />
                                    </DataGrid.ItemsSource>
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Binding="{Binding F1}" Header="依赖名" Width="*"/>
                                        <DataGridTextColumn Binding="{Binding F2}" Header="定义版本" Width="*"/>
                                        <DataGridTextColumn Binding="{Binding F3}" Header="安装版本" Width="*"/>
                                        <DataGridTextColumn Binding="{Binding F4}" Header="最新版本" Width="*"/>
                                        <DataGridTemplateColumn Header="操作" Width="*">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                        <Button Content="更新" Style="{StaticResource Link.Button}"/>
                                                        <Button Content="删除" Style="{StaticResource Link.Button}" Foreground="Red" Margin="10,0,0,0"/>
                                                    </StackPanel>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </TabItem>
                    </TabControl>

                </Grid>

            </Grid>
        </Grid>
    </Border>
</Window>

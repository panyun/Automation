<Application x:Class="WpfInspect.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfInspect">
    <!--StartupUri="MainWindow.xaml">-->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WpfInspect;component/Styles/WhiteTitleBarTheme.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WpfInspect;component/Styles/ListBox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WpfInspect;component/Styles/Treeview.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WpfInspect;component/Styles/ScrollViewer.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WpfInspect;component/Styles/ListView.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WpfInspect;component/Styles/Checkbox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

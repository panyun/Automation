<UserControl x:Class="Robot.Controls.DiagramControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Robot" xmlns:wpf="clr-namespace:Robot.Controls"
             mc:Ignorable="d" xmlns:controls1="http://schemas.panthernet.ru/graphx/"
             d:DesignHeight="450" d:DesignWidth="800">
    
    <Grid>
        <Grid.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="pack://application:,,,/EL.Robot.WpfMain;component/Controls/GraphTheme.xaml"/>
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        </Grid.Resources>
        <controls1:ZoomControl Margin="0" x:Name="tg_zoomctrl" Background="Transparent" Zoom="1">
            <wpf:GraphAreaExample x:Name="tg_Area"  Background="Transparent">

            </wpf:GraphAreaExample>
        </controls1:ZoomControl>
    </Grid>
</UserControl>

<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:attach="clr-namespace:EL.BaseUI.Themes"
                    >

    <Style TargetType="Border" x:Key="PopupBorderStyle">
        <Setter Property="BorderThickness" Value="1"></Setter>
        <Setter Property="BorderBrush" Value="{StaticResource Popup_BorderBrush}"></Setter>
        <Setter Property="Background" Value="{StaticResource Control_BackgroundBrush}"></Setter>
    </Style>
</ResourceDictionary>